#include "dog.h"
#include <stdio.h>

/**
 * dog_t new_dog - create a new dog
 * @name: char array
 * @age: float
 * @owner: char array
 * Return: dog_t
 */

dog_t *new_dog(char *name, float age, char *owner)
{
	dog_t *new_d;

	new_d->name = name;
	new_d->age = age;
	new_d->owner = owner;

	return (new_d);
}
