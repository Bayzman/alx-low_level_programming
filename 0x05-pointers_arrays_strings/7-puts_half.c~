#include "main.h"

/**
 * puts_half - print second half of any string
 * @str: char array
 * Return: void
 */

void puts_half(char *str)
{
	int i, length;

	for (i = 0; str[i] != '\0'; i++)
		continue;

	length = i;

	for (i = length; i >= length / 2; i--)
	{
		_putchar(str[i]);
	}
	_putchar('\n');
}
