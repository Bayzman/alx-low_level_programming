#include <stdio.h>
#include <stdlib.h>
#include "main.h"

/**
 * main - multiply two numbers
 * @argc: count of arguments
 * @argv: 1D vector of arguments
 * Return: int
 */

int main(int argc, char *argv[])
{
	int num1;
	int num2;
	int result;

	num1 = atoi(argv[1]);
	num2 = atoi(argv[2]);
	result = num1 * num2;

	if (argc < 3)
	{
		printf("Error\n");
		return (1);
	}
	else
	{
		printf("%d\n", result);
	}

	return (0);
}
